{
  "name": "shieldpro-ultimate",
  "version": "1.0.0",
  "description": "Advanced ad blocker with progressive tier system",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "webpack serve --mode development --config webpack.config.js",
    "build": "webpack --mode production",
    "build:prod": "NODE_ENV=production webpack --mode production --config webpack.config.js",
    "watch": "webpack --watch --mode development --config webpack.config.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.{ts,tsx}",
    "format": "prettier --write src/**/*.{ts,tsx,json,css}",
    "type-check": "tsc --noEmit",
    "package": "cd dist && zip -r ../extension.zip *",
    "release": "node scripts/release.js"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.3",
    "@radix-ui/react-dropdown-menu": "^2.1.3",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-select": "^2.1.3",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.3",
    "@radix-ui/themes": "^3.2.1",
    "@tailwindcss/postcss": "^4.1.12",
    "@tanstack/react-query": "^5.64.3",
    "axios": "^1.7.9",
    "clsx": "^2.1.1",
    "firebase": "^11.2.0",
    "i18next": "^25.4.0",
    "i18next-browser-languagedetector": "^8.0.3",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.473.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.2.1",
    "react-router-dom": "^7.8.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0",
    "typescript": "^5.9.2",
    "webextension-polyfill": "^0.12.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/chrome": "^0.0.290",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.9",
    "@types/react-dom": "^19.0.6",
    "@types/webextension-polyfill": "^0.12.1",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "autoprefixer": "^10.4.20",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.7",
    "eslint": "^9.20.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.4",
    "postcss": "^8.5.1",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.4.2",
    "process": "^0.11.10",
    "puppeteer": "^24.17.0",
    "sharp": "^0.34.3",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.12",
    "terser-webpack-plugin": "^5.3.11",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "keywords": [
    "chrome-extension",
    "adblocker",
    "react",
    "typescript",
    "firebase"
  ],
  "author": "ShieldPro Team",
  "license": "MIT"
}
