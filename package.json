{
  "name": "shieldpro-ultimate",
  "version": "1.0.0",
  "description": "Advanced ad blocker with progressive tier system",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "NODE_ENV=production webpack --mode production && node scripts/fix-build.cjs",
    "build:prod": "NODE_ENV=production webpack --mode production && node scripts/fix-build.cjs",
    "build:safe": "NODE_ENV=production webpack --mode production && node scripts/fix-build.cjs",
    "watch": "webpack --mode development --watch",
    "build:vite": "NODE_ENV=production vite build --mode production --config vite.config.ts",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest run --dir tests/unit",
    "test:integration": "vitest run --dir tests/integration",
    "test:e2e": "vitest run --dir tests/e2e",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "format": "prettier --write src/**/*.{ts,tsx,json,css}",
    "format:check": "prettier --check src/**/*.{ts,tsx,json,css}",
    "type-check": "tsc --noEmit",
    "clean:imports": "eslint src/**/*.{ts,tsx} --fix",
    "clean:all": "yarn format && yarn lint:fix",
    "fix": "yarn clean:all && yarn type-check",
    "check": "yarn type-check && yarn lint && yarn format:check",
    "package": "cd dist && zip -r ../extension.zip *",
    "release": "node scripts/release.js",
    "build:chrome": "BUILD_TARGET=chrome vite build",
    "build:firefox": "BUILD_TARGET=firefox vite build",
    "build:edge": "BUILD_TARGET=edge vite build",
    "build:all": "node scripts/build-browsers.js",
    "package:chrome": "yarn build:chrome && cd dist && zip -r ../builds/shieldpro-chrome.zip *",
    "package:firefox": "yarn build:firefox && cd dist && zip -r ../builds/shieldpro-firefox.zip *",
    "package:edge": "yarn build:edge && cd dist && zip -r ../builds/shieldpro-edge.zip *",
    "package:all": "yarn build:all",
    "clean": "rm -rf dist node_modules/.vite",
    "webpack:dev": "webpack serve --mode development --config webpack.config.cjs",
    "webpack:build": "webpack --mode production --config webpack.config.cjs",
    "dev:install": "node scripts/install-extension.js",
    "dev:reload": "node scripts/reload-extension.js",
    "analyze": "vite build --mode production && vite-bundle-visualizer",
    "prepare": "husky",
    "size": "size-limit",
    "precommit": "yarn type-check && yarn lint && yarn test:unit",
    "firebase:deploy:rules": "firebase deploy --only firestore:rules",
    "firebase:deploy:indexes": "firebase deploy --only firestore:indexes",
    "firebase:deploy:functions": "firebase deploy --only functions",
    "firebase:deploy:all": "firebase deploy --only firestore:rules,firestore:indexes,functions",
    "firebase:emulators": "firebase emulators:start",
    "firebase:backup": "node scripts/backup-firestore.js",
    "firebase:setup": "node scripts/setup-firebase.js"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.3",
    "@radix-ui/react-dropdown-menu": "^2.1.3",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-select": "^2.1.3",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.3",
    "@radix-ui/themes": "^3.2.1",
    "@tanstack/react-query": "^5.64.3",
    "axios": "^1.7.9",
    "clsx": "^2.1.1",
    "firebase": "^11.2.0",
    "i18next": "^25.4.0",
    "i18next-browser-languagedetector": "^8.0.3",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.473.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.2.1",
    "react-router-dom": "^7.8.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0",
    "typescript": "^5.9.2",
    "webextension-polyfill": "^0.12.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "2": "^3.0.0",
    "@crxjs/vite-plugin": "^2.2.0",
    "@playwright/test": "^1.55.0",
    "@size-limit/file": "^11.2.0",
    "@swc/core": "^1.13.5",
    "@swc/plugin-emotion": "^11.0.2",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/chrome": "^0.1.4",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.9",
    "@types/react-dom": "^19.0.6",
    "@types/webextension-polyfill": "^0.12.1",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "clean-webpack-plugin": "^4.0.0",
    "conventional-changelog-cli": "^5.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.7",
    "eslint": "^9.20.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "glob": "^11.0.3",
    "html-webpack-plugin": "^5.6.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.4",
    "postcss": "^8.5.1",
    "postcss-import": "^16.1.1",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.4.2",
    "process": "^0.11.10",
    "puppeteer": "^24.17.0",
    "sharp": "^0.34.3",
    "size-limit": "^11.2.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.0",
    "terser": "^5.43.1",
    "terser-webpack-plugin": "^5.3.11",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.4",
    "vite": "^7.1.3",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1"
  },
  "keywords": [
    "chrome-extension",
    "adblocker",
    "react",
    "typescript",
    "firebase"
  ],
  "author": "ShieldPro Team",
  "license": "MIT"
}
